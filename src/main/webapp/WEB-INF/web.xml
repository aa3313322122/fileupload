<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>fileupload</display-name>
  

  <servlet>
  	<servlet-name>uploadServlet</servlet-name>
  	<servlet-class>com.puban.util.UploadUtil</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>uploadServlet</servlet-name>
  	<url-pattern>/upload.action</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>progressServlet</servlet-name>
  	<servlet-class>com.puban.util.ProgressController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>progressServlet</servlet-name>
  	<url-pattern>/progress.action</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index2.jsp</welcome-file>
  </welcome-file-list>
</web-app>